{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    "apps/web": {
      "entry": [
        "app/**/page.{ts,tsx}",
        "app/**/layout.{ts,tsx}",
        "app/api/**/route.{ts,tsx}",
        "app/v1/**/route.{ts,tsx}",
        "middleware.{ts,tsx}",
        "middleware.cors.ts",
        "i18n.ts",
        "navigation.ts",
        "scripts/*.ts",
        "**/__tests__/**/*.{test,spec}.{ts,tsx}",
        "**/*.{test,spec}.{ts,tsx}"
      ],
      "project": ["**/*.{ts,tsx}", "scripts/**/*.ts"],
      "ignoreDependencies": [
        "@vm0/ui",
        "e2b",
        "@testing-library/react",
        "@types/tar",
        "@slack/web-api",
        "import-in-the-middle",
        "require-in-the-middle",
        "cpu-features"
      ],
      "next": {
        "entry": ["next.config.{js,ts,mjs}"]
      }
    },
    "apps/cli": {
      "entry": [
        "src/**/__tests__/**/*.{test,spec}.ts",
        "src/**/*.{test,spec}.ts"
      ],
      "project": ["src/**/*.ts"],
      "ignoreDependencies": ["@types/tar"],
      "tsup": {
        "config": ["tsup.config.ts"]
      }
    },
    "apps/docs": {
      "entry": ["source.config.ts"],
      "project": ["**/*.{ts,tsx,mdx,js,mjs}"],
      "ignoreDependencies": ["@vm0/typescript-config"]
    },
    "apps/platform": {
      "entry": ["src/main.ts", "**/*.{test,spec}.ts?(x)"],
      "project": ["**/*.{ts,tsx}"],
      "ignore": ["custom-eslint/**"],
      "ignoreDependencies": [
        "tailwindcss",
        "typescript-eslint",
        "@clerk/clerk-react"
      ]
    },
    "apps/runner": {
      "entry": [
        "src/**/__tests__/**/*.{test,spec}.ts",
        "src/**/*.{test,spec}.ts"
      ],
      "project": ["src/**/*.ts"],
      "tsup": {
        "config": ["tsup.config.ts"]
      }
    },
    "packages/core": {
      "entry": [
        "src/**/__tests__/**/*.{test,spec}.ts",
        "src/**/*.{test,spec}.ts"
      ],
      "project": ["src/**/*.ts"],
      "ignoreDependencies": ["yaml"]
    },
    "packages/sandbox-scripts": {
      "entry": [
        "src/**/__tests__/**/*.{test,spec}.ts",
        "src/**/*.{test,spec}.ts"
      ],
      "project": ["src/**/*.ts", "scripts/**/*.ts"],
      "ignore": ["src/scripts/**"]
    },
    "packages/ui": {
      "entry": [
        "src/**/__tests__/**/*.{test,spec}.{ts,tsx}",
        "src/**/*.{test,spec}.{ts,tsx}"
      ],
      "project": ["src/**/*.{ts,tsx}"],
      "ignoreDependencies": ["autoprefixer"]
    },
    "packages/eslint-config": {
      "entry": [],
      "project": ["**/*.js"]
    },
    "packages/typescript-config": {
      "entry": [],
      "project": ["*.json"],
      "ignoreUnresolved": ["next"]
    },
    "packages/proxy": {
      "entry": [],
      "project": ["**/*.{js,json}"]
    }
  },
  "ignore": [
    "**/*.d.ts",
    ".next/**",
    "dist/**",
    "build/**",
    "coverage/**",
    "**/drizzle.config.ts",
    "**/custom-eslint/**",
    "apps/platform/src/mocks/browser.ts",
    "apps/platform/src/signals/test-utils.ts",
    "apps/platform/src/signals/__tests__/test-helpers.ts",
    "apps/platform/src/signals/location.ts",
    "apps/platform/src/signals/log.ts",
    "apps/platform/src/signals/page-signal.ts",
    "apps/platform/src/signals/utils.ts",
    "apps/platform/src/signals/scope.ts",
    "apps/platform/src/views/router/link.tsx",
    "apps/platform/src/test/mocks/clerk-react.ts",
    "apps/platform/src/test/mocks/clerk-react-experimental.ts",
    "apps/runner/src/lib/firecracker/index.ts",
    "apps/runner/src/lib/firecracker/guest.ts",
    "apps/runner/src/lib/firecracker/network.ts",
    "apps/runner/src/lib/firecracker/netns-pool.ts",
    "apps/runner/src/lib/firecracker/client.ts",
    "apps/runner/src/lib/firecracker/vm.ts",
    "apps/web/src/test/setup.ts",
    "apps/web/src/lib/schedule/schedule-service.ts",
    "apps/web/src/lib/model-provider/model-provider-service.ts",
    "apps/cli/src/lib/api/api-client.ts",
    "apps/cli/src/lib/utils/paginate.ts",
    "apps/cli/src/commands/cook/cook.ts",
    "packages/core/src/contracts/public/agents.ts",
    "apps/web/src/lib/slack/**",
    "apps/web/src/lib/auth/sandbox-token.ts"
  ],
  "ignoreDependencies": ["@commitlint/cli"],
  "ignoreWorkspaces": [],
  "ignoreUnresolved": []
}
