{
  "name": "@vm0/cli",
  "version": "9.11.0",
  "private": true,
  "description": "CLI application",
  "repository": {
    "type": "git",
    "url": "https://github.com/vm0-ai/vm0",
    "directory": "turbo/apps/cli"
  },
  "type": "module",
  "bin": {
    "vm0": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "postbuild": "cp package.json dist/ && pnpm json -I -f dist/package.json -e 'this.name=\"@vm0/cli\"; delete this.private; delete this.scripts; delete this.devDependencies; delete this.dependencies[\"@vm0/core\"]; this.bin[\"vm0\"]=\"index.js\"; this.files=[\".\"]'",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "oxlint && eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@ts-rest/core": "3.53.0-rc.1",
    "@vm0/core": "workspace:*",
    "ably": "^2.17.0",
    "chalk": "^5.6.0",
    "commander": "^14.0.0",
    "dotenv": "^17.2.1",
    "prompts": "^2.4.2",
    "tar": "^7.5.2",
    "yaml": "^2.3.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/prompts": "^2.4.9",
    "@types/tar": "^6.1.13",
    "@vm0/eslint-config": "workspace:*",
    "@vm0/typescript-config": "workspace:*",
    "eslint": "^9.34.0",
    "json": "^11.0.0",
    "msw": "^2.12.7",
    "oxlint": "^1.14.0",
    "tsup": "^8.5.0",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  }
}
